<Project>
  <Import Project="Microsoft.AspNetCore.StaticWebAssetEndpoints.props" />
  <Import Project="Microsoft.AspNetCore.StaticWebAssets.props" />

  <!-- For Umbraco 17 (net10.0): Add V17 assets as Content items -->
  <ItemGroup Condition="'$(TargetFramework)' == 'net10.0' OR $(TargetFrameworks.Contains('net10.0'))">
    <Content Include="$(MSBuildThisFileDirectory)..\staticwebassets\BulkUpload\umbraco-package.json"
             Link="wwwroot\App_Plugins\BulkUpload\umbraco-package.json"
             CopyToOutputDirectory="PreserveNewest"
             CopyToPublishDirectory="PreserveNewest"
             Visible="false" />
    <Content Include="$(MSBuildThisFileDirectory)..\staticwebassets\BulkUpload\bulkupload.js"
             Link="wwwroot\App_Plugins\BulkUpload\bulkupload.js"
             CopyToOutputDirectory="PreserveNewest"
             CopyToPublishDirectory="PreserveNewest"
             Visible="false" />
    <Content Include="$(MSBuildThisFileDirectory)..\staticwebassets\BulkUpload\bulkupload.js.map"
             Link="wwwroot\App_Plugins\BulkUpload\bulkupload.js.map"
             CopyToOutputDirectory="PreserveNewest"
             CopyToPublishDirectory="PreserveNewest"
             Visible="false" />
  </ItemGroup>
</Project>
